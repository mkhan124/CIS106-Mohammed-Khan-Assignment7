            <declare name="discountedprice" type="Real" array="False" size=""/>
            <assign variable="discountedprice" expression="price - discountamount"/>
            <output expression="&quot;Quantity: &quot; &amp; qty &amp; &quot;, Total price: &quot; &amp; price &amp;&#13;&#10;&quot;, Discount amount: &quot; &amp; discountamount &amp; &quot;, Discounted price: &quot; &amp; discountedprice" newline="True"/>
