    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="qty" type="Real" array="False" size=""/>
            <declare name="price" type="Real" array="False" size=""/>
            <declare name="discountrate" type="Real" array="False" size=""/>
            <output expression="&quot;Enter quantity&quot;" newline="True"/>
            <input variable="qty"/>
            <output expression="&quot;Enter total price&quot;" newline="True"/>
            <input variable="price"/>
            <output expression="&quot;Enter discount rate&quot;" newline="True"/>
            <input variable="discountrate"/>
            <declare name="discountamount" type="Real" array="False" size=""/>
            <call expression="discountamount(discountrate, price)"/>
