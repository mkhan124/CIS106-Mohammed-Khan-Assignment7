    <function name="discountamount" type="Real" variable="discountamount">
        <parameters>
            <parameter name="discountrate" type="Real" array="False"/>
            <parameter name="price" type="Real" array="False"/>
        </parameters>
        <body>
            <declare name="discountamount" type="Real" array="False" size=""/>
            <assign variable="discountamount" expression="discountrate/100 * price"/>
        </body>
    </function>
